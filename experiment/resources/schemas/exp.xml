<ns:tables xsi:schemaLocation="http://labkey.org/data/xml ..\..\..\..\..\schemas\tableInfo.xsd" xmlns:ns="http://labkey.org/data/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ns:table tableName="Data" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="RowId">
        <ns:isKeyField>true</ns:isKeyField>
        <ns:description>Contains a unique id for this data object</ns:description>
      </ns:column>
      <ns:column columnName="LSID">
          <ns:description>Contains the life sciences identifier for this data object</ns:description>
      </ns:column>
      <ns:column columnName="Name">
          <ns:description>Contains a short name for this data object</ns:description>
      </ns:column>
      <ns:column columnName="Description">
          <ns:description>Contains a description for this data object</ns:description>
      </ns:column>
      <ns:column columnName="CpasType"/>
      <ns:column columnName="ClassId">
          <ns:description>Contains a reference to the DataClass this Data is a member of, if available</ns:description>
      </ns:column>
      <ns:column columnName="SourceApplicationId">
          <ns:description>Contains a reference to the ProtocolApplication that created this data object, if available</ns:description>
      </ns:column>
      <ns:column columnName="DataFileUrl">
          <ns:description>Contains the full path to this file on the server's file system</ns:description>
      </ns:column>
      <ns:column columnName="RunId">
          <ns:description>Contains a reference to the run that created this data object, if available</ns:description>
      </ns:column>
        <ns:column columnName="Created">
            <ns:description>Contains the date and time that this data was added to the system</ns:description>
        </ns:column>
        <ns:column columnName="CreatedBy">
            <ns:description>Contains the user that created this data</ns:description>
        </ns:column>
        <ns:column columnName="Modified">
            <ns:description>Contains the date and time that this data last modified</ns:description>
        </ns:column>
        <ns:column columnName="ModifiedBy">
            <ns:description>Contains the user that last modified this data</ns:description>
        </ns:column>
      <ns:column columnName="Container"/>
      <ns:column columnName="Generated">
          <ns:description>True if this ExpData File was generated by a PipelineJob process</ns:description>
      </ns:column>
    </ns:columns>
    <ns:description>Contains one row per data (typically a file with raw data or analysis results) object in this folder.</ns:description>
  </ns:table>
  <ns:table tableName="DataInput" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="DataId">
          <ns:description>The data object that was used as an input</ns:description>
      </ns:column>
      <ns:column columnName="TargetApplicationId">
          <ns:description>The protocol application that used the data as an input</ns:description>
      </ns:column>
      <ns:column columnName="Role">
          <ns:description>The purpose of the usage of the data object</ns:description>
      </ns:column>
    </ns:columns>
    <ns:description>Contains one row per data input in a protocol application for a run in this folder.</ns:description>
  </ns:table>
  <ns:table tableName="Experiment" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="RowId">
          <ns:description>Contains a unique id for this run group</ns:description>
      </ns:column>
      <ns:column columnName="LSID">
          <ns:description>Contains the life sciences identifier for this run</ns:description>
      </ns:column>
      <ns:column columnName="Name">
          <ns:description>Name for this run group.</ns:description>
          <ns:nullable>false</ns:nullable>
      </ns:column>
      <ns:column columnName="Hypothesis">
          <ns:description>An optional hypothesis for this run group if it represents an experiment.</ns:description>
      </ns:column>
      <ns:column columnName="ContactId">
          <ns:columnTitle>Contact</ns:columnTitle>
          <ns:description>An optional contact who is responsible for this run group.</ns:description>
      </ns:column>
      <ns:column columnName="ExperimentDescriptionURL">
          <ns:columnTitle>Description URL</ns:columnTitle>
          <ns:description>An optional URL that has further details about the purpose of this run group.</ns:description>
      </ns:column>
      <ns:column columnName="Comments">
          <ns:description>Optional extra information about this run group.</ns:description>
      </ns:column>
      <ns:column columnName="EntityId"/>
      <ns:column columnName="Created">
        <ns:description>Contains the time at which this run group was created</ns:description>
      </ns:column>
      <ns:column columnName="CreatedBy">
        <ns:fk>
          <ns:fkColumnName>UserId</ns:fkColumnName>
          <ns:fkTable>Users</ns:fkTable>
          <ns:fkDbSchema>core</ns:fkDbSchema>
        </ns:fk>
        <ns:description>Contains the user who created this run group</ns:description>
      </ns:column>
      <ns:column columnName="Modified">
        <ns:description>Contains the time at which this run group was last modified</ns:description>
      </ns:column>
      <ns:column columnName="ModifiedBy">
        <ns:fk>
          <ns:fkColumnName>UserId</ns:fkColumnName>
          <ns:fkTable>Users</ns:fkTable>
          <ns:fkDbSchema>core</ns:fkDbSchema>
        </ns:fk>
        <ns:description>Contains the user who created this run group</ns:description>
      </ns:column>
      <ns:column columnName="Container">
          <ns:description>Contains the folder or project in which this run group is stored</ns:description>
      </ns:column>
      <ns:column columnName="Hidden"/>
      <ns:column columnName="BatchProtocolId">
          <ns:description>Contains the protocol for which this group is a batch, or empty if this group is not a batch</ns:description>
      </ns:column>
    </ns:columns>
    <ns:description>Contains one row per run group, which consist of a collection of experiment runs.</ns:description>
  </ns:table>
  <ns:table tableName="ExperimentRun" tableDbType="TABLE">
      <ns:titleColumn>Name</ns:titleColumn>
    <ns:columns>
      <ns:column columnName="RowId">
          <ns:description>Contains a unique id for this run</ns:description>
      </ns:column>
      <ns:column columnName="Name">
          <ns:description>Contains a short name for this run</ns:description>
      </ns:column>
      <ns:column columnName="LSID">
        <ns:displayWidth>200</ns:displayWidth>
        <ns:description>Contains the life sciences identifier for this run</ns:description>
      </ns:column>
      <ns:column columnName="ProtocolLSID">
          <ns:description>Contains the protocol that this run follows</ns:description>
      </ns:column>
      <ns:column columnName="JobId">
          <ns:description>Contains the Id of the pipeline job that created this run, if applicable</ns:description>
      </ns:column>
      <ns:column columnName="Comments">
          <ns:inputRows>2</ns:inputRows>
          <ns:description>Contains comments about this run</ns:description>
      </ns:column>
      <ns:column columnName="EntityId"/>
      <ns:column columnName="Created">
        <ns:formatString>DateTime</ns:formatString>
        <ns:displayWidth>0</ns:displayWidth>
        <ns:description>Contains the time at which this protocol was last modified</ns:description>
      </ns:column>
      <ns:column columnName="CreatedBy">
        <ns:fk>
          <ns:fkColumnName>UserId</ns:fkColumnName>
          <ns:fkTable>Users</ns:fkTable>
          <ns:fkDbSchema>core</ns:fkDbSchema>
        </ns:fk>
        <ns:description>Contains the user who created this run</ns:description>
      </ns:column>
      <ns:column columnName="Modified">
        <ns:description>Contains the time at which this protocol was last modified</ns:description>
      </ns:column>
      <ns:column columnName="ModifiedBy">
        <ns:fk>
          <ns:fkColumnName>UserId</ns:fkColumnName>
          <ns:fkTable>Users</ns:fkTable>
          <ns:fkDbSchema>core</ns:fkDbSchema>
        </ns:fk>
        <ns:description>Contains the user who last modified this run</ns:description>
      </ns:column>
      <ns:column columnName="Container">
        <ns:description>Contains the folder or project in which this run is stored</ns:description>
      </ns:column>
      <ns:column columnName="FilePathRoot">
        <ns:description>Contains the directory on the server's file system where this run's data files come from</ns:description>
      </ns:column>
      <ns:column columnName="ReplacedByRunId">
        <ns:description>The run that has replaced this run, usually with updated or corrected information</ns:description>
      </ns:column>
      <ns:column columnName="BatchId">
        <ns:description>Contains the associated batch or empty if this run is not part of a batch.</ns:description>
      </ns:column>
    </ns:columns>
    <ns:description>Contains a row per experiment run in this folder.</ns:description>
  </ns:table>
  <ns:table tableName="List" tableDbType="TABLE">
      <ns:buttonBarOptions position="top" includeStandardButtons="true">
      </ns:buttonBarOptions>
      <ns:description>Contains one row for every list managered in this container.</ns:description>
      <ns:tableUrl>/list/grid.view?listId=${ListId}</ns:tableUrl>
      <ns:titleColumn>Name</ns:titleColumn>
      <ns:pkColumnName>ListId</ns:pkColumnName>
      <ns:columns>
          <ns:column columnName="ListId" >
            <ns:isKeyField>true</ns:isKeyField>
            <ns:isHidden>true</ns:isHidden>
          </ns:column>
          <ns:column columnName="EntityId" />
          <ns:column columnName="Created" >
              <ns:isHidden>true</ns:isHidden>
          </ns:column>
          <ns:column columnName="CreatedBy" >
              <ns:isHidden>true</ns:isHidden>
          </ns:column>
          <ns:column columnName="Modified" >
              <ns:isHidden>true</ns:isHidden>
          </ns:column>
          <ns:column columnName="ModifiedBy" >
              <ns:isHidden>true</ns:isHidden>
          </ns:column>
          <ns:column columnName="Container" />
          <ns:column columnName="Name" >
              <ns:url>/list/grid.view?listId=${ListId}</ns:url>
          </ns:column>
          <ns:column columnName="DomainId" />
          <ns:column columnName="KeyName" />
          <ns:column columnName="KeyType" />
          <ns:column columnName="TitleColumn" /> 
          <ns:column columnName="Description" />
          <ns:column columnName="DiscussionSetting" />
          <ns:column columnName="AllowDelete" />
          <ns:column columnName="AllowUpload" />
          <ns:column columnName="AllowExport" />
          <ns:column columnName="LastIndexed"/>
          <ns:column columnName="EntireListIndex"/>
          <ns:column columnName="EntireListIndexSetting"/>
          <ns:column columnName="EntireListTitleSetting" />
          <ns:column columnName="EntireListTitleTemplate"/>
          <ns:column columnName="EntireListBodySetting" />
          <ns:column columnName="EntireListBodyTemplate"/>
          <ns:column columnName="EachItemIndex"/>
          <ns:column columnName="EachItemTitleSetting" />
          <ns:column columnName="EachItemTitleTemplate"/>
          <ns:column columnName="EachItemBodySetting" />
          <ns:column columnName="EachItemBodyTemplate"/>
      </ns:columns>
  </ns:table>
    <ns:table tableName="Material" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="RowId">
          <ns:description>Contains the unique identifier for this sample</ns:description>
      </ns:column>
      <ns:column columnName="LSID">
          <ns:description>Contains a life sciences identifier for this sample</ns:description>
      </ns:column>
      <ns:column columnName="Name">
          <ns:description>Contains a short description for this sample</ns:description>
      </ns:column>
      <ns:column columnName="CpasType">
          <ns:columnTitle>Sample Set</ns:columnTitle>
          <ns:description>If this sample is a member of a sample set, contains a reference to the sample set</ns:description>
      </ns:column>
      <ns:column columnName="SourceApplicationId">
          <ns:description>Contains a reference to the protocol application that created this sample, if available</ns:description>
      </ns:column>
      <ns:column columnName="RunId">
          <ns:description>Contains a reference to the experiment run that created this sample, if available</ns:description>
      </ns:column>
      <ns:column columnName="Created">
          <ns:description>Contains the date and time that this sample was added to the system</ns:description>
      </ns:column>
      <ns:column columnName="CreatedBy">
          <ns:description>Contains the user that created this sample</ns:description>
      </ns:column>
      <ns:column columnName="Modified">
          <ns:description>Contains the date and time that this sample last modified</ns:description>
      </ns:column>
      <ns:column columnName="ModifiedBy">
          <ns:description>Contains the user that last modified this sample</ns:description>
      </ns:column>
      <ns:column columnName="Container">
          <ns:description>Contains a reference to the folder that holds this sample</ns:description>
      </ns:column>
      <ns:column columnName="LastIndexed">
          <ns:description>Contains the time this material was last updated in the search index</ns:description>
      </ns:column>
    </ns:columns>
    <ns:description>Contains a row per material or sample described in this folder.</ns:description>
  </ns:table>
  <ns:table tableName="MaterialInput" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="MaterialId">
        <ns:description>The material object that was used as an input</ns:description>
      </ns:column>
      <ns:column columnName="TargetApplicationId">
        <ns:description>The protocol application that used the data as an input</ns:description>
      </ns:column>
      <ns:column columnName="Role">
        <ns:description>The purpose of the usage of the data object</ns:description>
      </ns:column>
    </ns:columns>
    <ns:description>Contains one row per material input in a protocol application for a run in this folder.</ns:description>
  </ns:table>
  <ns:table tableName="MaterialSource" tableDbType="TABLE">
    <ns:gridUrl>/experiment/listMaterialSources.view</ns:gridUrl>
    <ns:tableUrl>/experiment/showMaterialSource.view?rowId=${RowId}</ns:tableUrl>
    <ns:columns>
      <ns:column columnName="RowId">
          <ns:description>Contains a unique id for this sample set</ns:description>
      </ns:column>
      <ns:column columnName="Name">
          <ns:description>Contains a short name for this sample set</ns:description>
      </ns:column>
      <ns:column columnName="LSID">
          <ns:isReadOnly>true</ns:isReadOnly>
          <ns:description>Contains a life sciences identifier for this protocol</ns:description>
      </ns:column>
      <ns:column columnName="MaterialLSIDPrefix">
          <ns:isReadOnly>true</ns:isReadOnly>
          <ns:description>Contains the LSID prefix for all materials that are part of this sample set</ns:description>
      </ns:column>
      <ns:column columnName="Description">
          <ns:description>Contains an optional description about the contents or purpose of this sample set</ns:description>
      </ns:column>
      <ns:column columnName="Created">
          <ns:description>Contains the time at which this sample set was created</ns:description>
      </ns:column>
      <ns:column columnName="IdCol1"/>
      <ns:column columnName="IdCol2"/>
      <ns:column columnName="IdCol3"/>
      <ns:column columnName="ParentCol"/>
      <ns:column columnName="CreatedBy">
        <ns:fk>
          <ns:fkColumnName>UserId</ns:fkColumnName>
          <ns:fkTable>Users</ns:fkTable>
          <ns:fkDbSchema>core</ns:fkDbSchema>
        </ns:fk>
        <ns:description>Contains the user that created this sample set</ns:description>
      </ns:column>
      <ns:column columnName="Modified">
          <ns:description>Contains the time at which this sample set was last modified</ns:description>
      </ns:column>
      <ns:column columnName="ModifiedBy">
        <ns:fk>
          <ns:fkColumnName>UserId</ns:fkColumnName>
          <ns:fkTable>Users</ns:fkTable>
          <ns:fkDbSchema>core</ns:fkDbSchema>
        </ns:fk>
        <ns:description>Contains the user that last modified this sample set</ns:description>
      </ns:column>
      <ns:column columnName="Container">
          <ns:description>Contains the folder or project in which this sample set is stored</ns:description>
      </ns:column>
    </ns:columns>
    <ns:description>Contains a row per sample set, a collection of samples.</ns:description>
  </ns:table>
  <ns:table tableName="DataClass" tableDbType="TABLE">
    <ns:gridUrl>/experiment/listDataClass.view</ns:gridUrl>
    <ns:columns>
      <ns:column columnName="RowId">
        <ns:description>Contains a unique id for this DataClass</ns:description>
        <ns:isHidden>true</ns:isHidden>
      </ns:column>
      <ns:column columnName="Name"/>
      <ns:column columnName="LSID"/>
      <ns:column columnName="Container"/>
      <ns:column columnName="Created"/>
      <ns:column columnName="CreatedBy"/>
      <ns:column columnName="Modified"/>
      <ns:column columnName="ModifiedBy"/>
      <ns:column columnName="Description"/>
      <ns:column columnName="MaterialSourceId"/>
      <ns:column columnName="NameExpression"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="PropertyDescriptor" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="PropertyId"/>
      <ns:column columnName="PropertyURI"/>
      <ns:column columnName="OntologyURI"/>
      <ns:column columnName="RangeURI"/>
      <ns:column columnName="Name"/>
      <ns:column columnName="StorageColumnName"/>
      <ns:column columnName="Description"/>
      <ns:column columnName="ConceptURI"/>
      <ns:column columnName="Label"/>
      <ns:column columnName="SearchTerms"/>
      <ns:column columnName="SemanticType"/>
      <ns:column columnName="Format"/>
      <ns:column columnName="Container"/>
      <ns:column columnName="Project"/>
      <ns:column columnName="LookupContainer"/>
      <ns:column columnName="LookupSchema"/>
      <ns:column columnName="LookupQuery"/>
      <ns:column columnName="MvEnabled"/>
      <ns:column columnName="Hidden"/>
      <ns:column columnName="DefaultValueType"/>
      <ns:column columnName="ImportAliases"/>
      <ns:column columnName="URL"/>
      <ns:column columnName="ShownInInsertView"/>
      <ns:column columnName="ShownInUpdateView"/>
      <ns:column columnName="ShownInDetailsView"/>
      <ns:column columnName="Dimension"/>
      <ns:column columnName="Measure"/>
      <ns:column columnName="RecommendedVariable"/>
      <ns:column columnName="DefaultScale"/>
      <ns:column columnName="FacetingBehaviorType"/>
      <ns:column columnName="Protected"/>
      <ns:column columnName="ExcludeFromShifting"/>
      <ns:column columnName="Created"/>
      <ns:column columnName="CreatedBy"/>
      <ns:column columnName="Modified"/>
      <ns:column columnName="ModifiedBy"/>
      <ns:column columnName="Scale"/>
  </ns:columns>
  </ns:table>
  <ns:table tableName="DomainDescriptor" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="DomainId"/>
      <ns:column columnName="DomainURI"/>
      <ns:column columnName="Name"/>
      <ns:column columnName="Description"/>
      <ns:column columnName="Container"/>
      <ns:column columnName="Project"/>
      <ns:column columnName="StorageTableName"/>
      <ns:column columnName="StorageSchemaName"/>
      <ns:column columnName="Modified"/>
      <ns:column columnName="ModifiedBy"/>
      <ns:column columnName="_ts"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="PropertyDomain" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="PropertyId"/>
      <ns:column columnName="DomainId"/>
      <ns:column columnName="Required"/>
      <ns:column columnName="SortOrder"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="Object" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="ObjectId"/>
      <ns:column columnName="Container"/>
      <ns:column columnName="ObjectUri"/>
      <ns:column columnName="OwnerObjectId"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="ObjectProperty" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="ObjectId"/>
      <ns:column columnName="PropertyId"/>
      <ns:column columnName="TypeTag"/>
      <ns:column columnName="FloatValue"/>
      <ns:column columnName="DateTimeValue"/>
      <ns:column columnName="StringValue"/>
      <ns:column columnName="MvIndicator"/>  
    </ns:columns>
  </ns:table>
  <ns:table tableName="Protocol" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="RowId">
          <ns:description>Contains the unique id for this protocol</ns:description>
      </ns:column>
      <ns:column columnName="LSID">
          <ns:description>Contains a life sciences identifier for this protocol</ns:description>
      </ns:column>
      <ns:column columnName="Name">
          <ns:description>Contains a short name for this protocol</ns:description>
      </ns:column>
      <ns:column columnName="ProtocolDescription"/>
      <ns:column columnName="ApplicationType"/>
      <ns:column columnName="MaxInputMaterialPerInstance"/>
      <ns:column columnName="MaxInputDataPerInstance"/>
      <ns:column columnName="OutputMaterialPerInstance"/>
      <ns:column columnName="OutputDataPerInstance"/>
      <ns:column columnName="OutputMaterialType"/>
      <ns:column columnName="OutputDataType"/>
      <ns:column columnName="Instrument"/>
      <ns:column columnName="Software"/>
      <ns:column columnName="ContactId"/>
      <ns:column columnName="Created">
        <ns:description>Contains the time at which this protocol was created</ns:description>
      </ns:column>
      <ns:column columnName="EntityId"/>
      <ns:column columnName="CreatedBy">
        <ns:fk>
          <ns:fkColumnName>UserId</ns:fkColumnName>
          <ns:fkTable>Users</ns:fkTable>
          <ns:fkDbSchema>core</ns:fkDbSchema>
        </ns:fk>
        <ns:description>Contains the user that created this protocol</ns:description>
      </ns:column>
      <ns:column columnName="Modified">
        <ns:description>Contains the time at which this protocol was last modified</ns:description>
      </ns:column>
      <ns:column columnName="ModifiedBy">
        <ns:fk>
          <ns:fkColumnName>UserId</ns:fkColumnName>
          <ns:fkTable>Users</ns:fkTable>
          <ns:fkDbSchema>core</ns:fkDbSchema>
        </ns:fk>
        <ns:description>Contains the user that last modified this protocol</ns:description>
      </ns:column>
      <ns:column columnName="Container">
          <ns:description>Contains the folder or project where this protocol is stored</ns:description>
      </ns:column>
    </ns:columns>
    <ns:description>Contains a row per experiment protocol, a series of steps that may be performed when doing an experiment run.</ns:description>
  </ns:table>
  <ns:table tableName="ProtocolAction" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="RowId"/>
      <ns:column columnName="ParentProtocolId"/>
      <ns:column columnName="ChildProtocolId"/>
      <ns:column columnName="Sequence"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="ProtocolActionPredecessor" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="ActionId"/>
      <ns:column columnName="PredecessorId"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="ProtocolApplication" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="RowId">
          <ns:description>Contains the unique id for this protocol application</ns:description>
      </ns:column>
      <ns:column columnName="LSID">
          <ns:description>Contains a life sciences identifier for this protocol application</ns:description>
      </ns:column>
      <ns:column columnName="Name">
          <ns:description>Contains the short name for this protocol application</ns:description>
      </ns:column>
      <ns:column columnName="CpasType">
          <ns:description>Contains the type of step - ExperimentRun (for inputs), ProtocolApplication (for a processing step), or ExperimentRunOutput (for outputs)</ns:description>
      </ns:column>
      <ns:column columnName="ProtocolLSID">
          <ns:description>Contains the protocol step that was performed</ns:description>
      </ns:column>
      <ns:column columnName="ActivityDate">
          <ns:description>Contains the date this step was performed</ns:description>
      </ns:column>
      <ns:column columnName="Comments">
          <ns:description>Contains an optional comment about what was done</ns:description>
      </ns:column>
      <ns:column columnName="RunId">
          <ns:description>Contains a reference to the run this step was a part of</ns:description>
      </ns:column>
      <ns:column columnName="ActionSequence">
          <ns:description>Contains an index of when this step was performed within the run</ns:description>
      </ns:column>
      <ns:column columnName="StartTime">
          <ns:formatString>HH:mm:ss.SSS</ns:formatString>
          <ns:description>Contains the time this step was started</ns:description>
      </ns:column>
      <ns:column columnName="EndTime">
          <ns:formatString>HH:mm:ss.SSS</ns:formatString>
          <ns:description>Contains the time this step was finished</ns:description>
      </ns:column>
      <ns:column columnName="RecordCount">
          <ns:description>Contains the number of records affected by this step</ns:description>
      </ns:column>
    </ns:columns>
    <ns:description>Contains a row per step taken when performing an experiment run.</ns:description>
  </ns:table>
