{
  "private": true,
  "scripts": {
    "setup": "npm install",
    "build": "npm run clean && better-npm-run build:prod",
    "start": "better-npm-run build:watch",
    "build-prod": "npm run build",
    "clean": "better-npm-run clean"
  },
  "betterScripts": {
    "build:watch": {
      "command": "webpack-dev-server --config webpack/dev.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "webpack --config webpack/prod.config.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "clean": {
      "command": "rimraf resources/web/experiment/gen && rimraf resources/views/*.*"
    }
  },
  "dependencies": {
    "@glass/domainproperties": "0.0.2",
    "@glass/models": "0.0.3",
    "@glass/utils": "0.0.4",
    "@labkey/api": "0.0.9",
    "@types/react": "16.4.18",
    "@types/react-dom": "16.0.9",
    "font-awesome": "4.7.0",
    "immutable": "3.8.2",
    "jquery": "3.3.1",
    "react": "16.7.0",
    "react-bootstrap": "0.32.4",
    "react-dom": "16.7.0",
    "reactn": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@types/react-hot-loader": "4.1.0",
    "babel-loader": "8.0.5",
    "better-npm-run": "0.1.1",
    "html-webpack-plugin": "3.2.0",
    "react-hot-loader": "4.6.5",
    "ts-loader": "5.3.3",
    "typescript": "3.1.6",
    "webpack": "4.29.1",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14"
  }
}
