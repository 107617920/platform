<ns:tables xsi:schemaLocation="http://labkey.org/data/xml ..\..\..\..\..\schemas\tableInfo.xsd" xmlns:ns="http://labkey.org/data/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <ns:table tableName="AuditLog" tableDbType="TABLE">
      <ns:description>Contains a row for each audit event that occurred in this folder.</ns:description>
      <ns:columns>
        <ns:column columnName="RowId"/>
        <ns:column columnName="Key1"/>
        <ns:column columnName="Key2"/>
        <ns:column columnName="Key3"/>
        <ns:column columnName="IntKey1"/>
        <ns:column columnName="IntKey2"/>
        <ns:column columnName="IntKey3"/>
        <ns:column columnName="Comment">
          <ns:displayWidth>400</ns:displayWidth>
        </ns:column>
        <ns:column columnName="CreatedBy"/>
        <ns:column columnName="Created">
          <ns:formatString>DateTime</ns:formatString>
          <ns:displayWidth>120</ns:displayWidth>
          <ns:textAlign>left</ns:textAlign>
        </ns:column>
        <ns:column columnName="EventType"/>
        <ns:column columnName="ProjectId"/>
        <ns:column columnName="ContainerId"/>
        <ns:column columnName="EntityId"/>
        <ns:column columnName="Lsid"/>
        <ns:column columnName="ImpersonatedBy"/>
      </ns:columns>
    </ns:table>
</ns:tables>