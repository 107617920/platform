{
  "name": "labkey-core",
  "version": "0.0.0",
  "scripts": {
    "clean": "bnr clean",
    "build": "npm run clean && bnr build:themes",
    "build-prod": "npm run build",
    "prebuild": "npm run clean",
    "setup": "npm install"
  },
  "betterScripts": {
    "clean": {
      "command": "rimraf resources/web/core/css"
    },
    "build:themes": {
      "command": "node webpack/buildThemes.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "7.1.0",
    "better-npm-run": "0.0.15",
    "bootstrap-sass": "3.3.7",
    "css-loader": "0.28.1",
    "cssnano": "3.10.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "font-awesome": "4.7.0",
    "node-sass": "4.5.3",
    "postcss-loader": "1.3.3",
    "precss": "1.4.0",
    "resolve-url-loader": "2.0.2",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.5",
    "style-loader": "0.17.0",
    "url-loader": "0.5.8",
    "webpack": "2.5.1"
  }
}
