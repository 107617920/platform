{
  "name": "labkey-core",
  "version": "0.0.0",
  "scripts": {
    "clean": "bnr clean",
    "build": "bnr build:style",
    "build-prod": "npm run build",
    "prebuild": "npm run clean",
    "setup": "npm install"
  },
  "betterScripts": {
    "clean": {
      "command": "rimraf resources/web/core/css"
    },
    "build:style": {
      "command": "webpack --config webpack/style.config.js --progress",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "6.6.1",
    "better-npm-run": "0.0.14",
    "bootstrap-sass": "3.3.7",
    "css-loader": "0.26.1",
    "cssnano": "3.10.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "font-awesome": "4.7.0",
    "node-sass": "4.3.0",
    "postcss-loader": "1.2.2",
    "precss": "1.4.0",
    "resolve-url-loader": "1.6.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0"
  }
}
