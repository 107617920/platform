      </ns:column>
      <ns:column columnName="OwnerId">
        <ns:datatype>uniqueidentifier</ns:datatype>
        <ns:columnTitle>Owner Id</ns:columnTitle>
        <ns:inputType>text</ns:inputType>
        <ns:inputLength>36</ns:inputLength>
      </ns:column>
      <ns:column columnName="Name">
        <ns:columnTitle>Name</ns:columnTitle>
        <ns:inputType>text</ns:inputType>
        <ns:scale>255</ns:scale>
        <ns:inputLength>40</ns:inputLength>
        <ns:isReadOnly>false</ns:isReadOnly>
        <ns:description>The name of this principal.</ns:description>
      </ns:column>
      <ns:column columnName="Container">
        <ns:description>The container in which this principal is defined. For users this is null, but for project groups, this is the id of the project in which the group is defined.</ns:description>
      </ns:column>
      <ns:column columnName="Type">
        <ns:description>The type of this principal (u=user; g=project group; r=role; m=module-defined group)</ns:description>
      </ns:column>
      <ns:column columnName="Active">
        <ns:description>True if this principal is active, false if not. Inactive principals are not allowed to sign-in.</ns:description>
      </ns:column>
    </ns:columns>
    <ns:tableTitle>Principals</ns:tableTitle>
